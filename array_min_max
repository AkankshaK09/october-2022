import java.lang.reflect.Array;
import java.util.*;
public class array_min_max {

	public static void main(String[] args) {
		Integer [] arr = {1,3,55,3,5,6};
		int a = Collections.min(Arrays.asList(arr));
		System.out.println(a);
		
		String [] arra = {"aac", "asfdga", "srgasv", "asrgasd"};
		String b = Collections.min(Arrays.asList(arra));
		System.out.println(b);
	}

}
